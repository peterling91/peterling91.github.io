{
  "name": "nyp-pet",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npx webpack --watch",
    "build": "npx webpack build --mode=production",
    "tailwind": "npx tailwindcss -i src/styles/index.css -o dist/index.css --minify --watch",
    "tailwind-xs": "npx tailwindcss -i src/styles/index.css -o dist/index.css --minify --watch",
    "tailwind-pcm": "npx tailwindcss -i src/styles/section-how-you-learn.css -o dist/section-how-you-learn.css --minify --watch",
    "tailwind-persona": "npx tailwindcss -i src/styles/persona-page.css -o dist/persona-page.css --minify --watch",
    "tailwind-confetti": "npx tailwindcss -i src/styles/confetti.css -o dist/confetti.css --minify --watch",
    "tailwind-map": "npx tailwindcss -i src/styles/twodmap.css -o dist/twodmap.css --minify --watch",
    "tailwind-event-details": "npx tailwindcss -i src/styles/section-event-details.css -o dist/section-event-details.css --minify --watch",
    "prettier": "npx prettier src --write",
    "dev": "concurrently 'npm run start' 'npm run tailwind' 'npm run tailwind-pcm' 'npm run tailwind-persona' 'npm run tailwind-confetti' 'npm run tailwind-map' 'npm run tailwind-event-details'",
    "production": "concurrently 'npm run build' 'npm run tailwind' 'npm run tailwind-pcm' 'npm run tailwind-persona' 'npm run tailwind-confetti' 'npm run tailwind-map' 'npm run tailwind-event-details' 'npm run prettier'",
    "uglify-fmo": "uglifyjs src/js/fmo-confetti.js -o dist/fmo-confetti.js -c -m",
    "uglify-grad": "uglifyjs src/js/graduation-confetti.js -o dist/graduation-confetti.js -c -m",
    "uglify": "concurrently 'npm run uglify-fmo' 'npm run uglify-grad'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.197",
    "autoprefixer": "^10.4.15",
    "lodash": "^4.17.21",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^2.0.2",
    "@popperjs/core": "^2.11.8",
    "concurrently": "^8.2.1",
    "gsap": "^3.12.5",
    "lazysizes": "^5.3.2",
    "lenis": "^1.1.13",
    "uglify-js": "^3.17.4"
  }
}
